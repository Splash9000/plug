<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="divemce.com.divecalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dcalc"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".calculations" android:label="@string/ab_calculations_label" />
        <activity android:name=".bestmix" android:label="@string/ab_bestmix_label" />
        <activity android:name=".mod" android:label="@string/ab_mod_label" />
        <activity android:name=".ppo2" android:label="@string/ab_ppo2_label" />
        <activity android:name=".ead" android:label="@string/ab_ead_label"></activity>
    </application>

</manifest>